{
  "name": "steady-links",
  "version": "0.1.0",
  "description": "Edit links in a modal and reposition cursor safely",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc --noEmit && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin"
  ],
  "author": "Scott Otterson",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^4.0.0",
    "@vitest/ui": "^4.0.0",
    "builtin-modules": "^4.0.0",
    "esbuild": "^0.19.10",
    "fast-check": "^4.5.3",
    "obsidian": "latest",
    "prettier": "^3.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.0"
  }
}
